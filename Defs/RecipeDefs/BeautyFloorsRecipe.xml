<Defs>
  <RecipeDef>
    <defName>MakeWoodPlanks</defName>
    <label>make wood planks</label>
    <description>Splitting logs to planks for make floors.</description>
    <jobString>Splitting logs.</jobString>
    <workAmount>100</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <workSkill>Crafting</workSkill>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>6</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <WoodPlanks>6</WoodPlanks>
    </products>
    <skillRequirements>
      <Crafting>5</Crafting>
    </skillRequirements>
  </RecipeDef>
